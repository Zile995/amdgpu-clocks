# Set custom GPU states 4, 5, 6 & 7:
OD_SCLK:
4:       1215MHz        1130mV
5:       1257MHz        1125mV
6:       1300MHz        1125mV
7:       1500MHz        1155mV
# Set custom memory states 1 & 2:
OD_MCLK:
2:       1850MHz        950mV
# Only allow SCLK states 7:  
# FORCE_SCLK: 7
# Force fixed memory state:
# FORCE_MCLK: 2
# Force power limit (in micro watts):
FORCE_POWER_CAP: 200000000
# In order to allow FORCE_SCLK & FORCE_MCLK:
FORCE_PERF_LEVEL: auto
